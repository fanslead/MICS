.container { padding: 16px; }
.row { display: flex; flex-direction: row; align-items: center; margin-bottom: 8px; }
.label { width: 70px; font-size: 12px; color: #666; }
.input { flex: 1; border: 1px solid #ddd; padding: 6px 8px; border-radius: 4px; font-size: 12px; }
.buttons { display: flex; gap: 8px; margin-top: 8px; }
.log { margin-top: 12px; font-size: 12px; color: #333; white-space: pre-wrap; }

